{% macro loading() %}
    <script type="text/javascript" src="{{ asset('back_template/assets/js/plugins/loaders/progressbar.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('back_template/assets/js/pages/components_loaders.js') }}"></script>
    <script>
        function loadingBlock()
        {
            $(".loadingBlock").block({
                message: '<i class="icon-spinner4 spinner"></i>',
                overlayCSS: {
                    backgroundColor: '#fff',
                    opacity: 0.8,
                    cursor: 'wait'
                },
                css: {
                    border: 0,
                    padding: 0,
                    backgroundColor: 'none'
                }
            });
        }
        function unloadingBlock()
        {
            $(".loadingBlock").unblock();
        }
    </script>
{% endmacro %}
