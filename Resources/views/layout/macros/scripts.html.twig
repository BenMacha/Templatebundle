{% macro dataTable(path=null,columns=null) %}
    <script type="text/javascript" src="{{ asset('back_template/assets/js/plugins/tables/datatables/datatables.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('back_template/assets/js/plugins/forms/selects/select2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('back_template/assets/js/pages/datatables_data_sources.js') }}"></script>
    <script type="text/javascript" src="{{ asset('back_template/assets/js/pages/components_popups.js') }}"></script>
    {% if path is not null %}
        <script>
            var dataTable;
            $(function () {
                dataTable = $('#myTable').DataTable({
                    ajax: {
                        url: '{{ path(path) }}',
                        type: "POST"
                    },
                    "pageLength": 25,
                    columns: [{{ columns|raw }}]
                });
                $('.dataTables_length select').select2({
                    minimumResultsForSearch: Infinity,
                    width: 'auto'
                });
                $('.dataTables_filter input[type=search]').attr('placeholder','Type to filter...');
            });
        </script>
    {% endif %}
{% endmacro %}
